# the polar()function  
          
# importing "cmath"  
# for mathematical operations  
import cmath  
      
# using cmath.polar() method  
num = cmath.polar(1)  
print(num) 
       
       
       #Using cmath.phase (z)


import cmath 
  
  
x = -1.0
y = 0.0
z = complex(x,y);  
  
# printing phase of a complex number using phase()  
print ("The phase of complex number is : ",end="")  
print (cmath.phase(z)) 
